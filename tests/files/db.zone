$TTL 604800
@ IN SOA home. root.home. (
                 2         ; Serial
            604800         ; Refresh
             86400         ; Retry
           2419200         ; Expire
            604800 )       ; Negative Cache TTL
;

;
; Name-servers
;
@ IN NS dns1
@ IN NS dns2
;
; All servers
;
dns1 IN A 127.0.0.5
dns2 IN A 127.0.0.6
www IN A 127.0.1.5
